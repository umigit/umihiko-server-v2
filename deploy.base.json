{
  "containers": {
    "nginx": {
      "image": "<nginx-image>",
      "environment": {},
      "ports": {
        "80": "HTTP"
      }
    },
    "pgadmin4": {
      "image": "dpage/pgadmin4",
      "environment": {
        "PGADMIN_DEFAULT_EMAIL": "<PGADMIN_DEFAULT_EMAIL>",
        "PGADMIN_DEFAULT_PASSWORD": "<PGADMIN_DEFAULT_PASSWORD>",
        "PGADMIN_LISTEN_PORT": "5433"
      },
      "ports": {
        "5433": "HTTP"
      }
    }
  },
  "publicEndpoint": {
    "containerName": "nginx",
    "containerPort": 80,
    "healthCheck": {
      "healthyThreshold": 2,
      "unhealthyThreshold": 2,
      "timeoutSeconds": 5,
      "intervalSeconds": 10,
      "path": "/healthcheck.html",
      "successCodes": "200-499"
    }
  }
}
